---
import CASEY_FACTS from "@/src/data/facts.json";
import Page from "@/src/layouts/Page.astro";

export async function getStaticPaths() {
  return CASEY_FACTS.map((fact) => ({
    params: { slug: fact.slug },
    props: { fact },
  }));
}

const { fact } = Astro.props;
---

<Page>
  {fact.content}
</Page>
