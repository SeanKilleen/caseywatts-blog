	// .filter((episode) => !episode.data.draft);
import MaintainablePodcast from "../../MaintainablePodcast.xml?raw";
const podcastData = podparse(MaintainablePodcast);
---

<SEOTags
	title="Casey Watts! The Podcast"
	description="Casey writes about: programming, psychology, workplace culture, community development, personal updates, tech tips, local DMV-area events and online events."
	contentType="website"
	url={Astro.url.href}
/>

<OuterLayout>
	<PodcastEpisodesIndex podcastData={podcastData} client:load />
</OuterLayout>
