---
import podparse from "podparse";
import OuterLayout from '../../layouts/OuterLayout.astro';
import SEOTags from '../../components/SEOTags.svelte';
import PodcastEpisodesIndex from '../../components/PodcastEpisodesIndex.svelte';

// import GTCEpisodes from "../../GreaterThanCodePodcastRSS.xml?raw";
// const podcastData = podparse(GTCEpisodes);
